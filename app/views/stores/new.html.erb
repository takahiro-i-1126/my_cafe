<% provide(:title, '店の登録') %>
<h1>店の登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@store) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :intorduction %>
      <%= f.text_field :introduction, size: "100x50"%>

      <%= f.submit "登録", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
